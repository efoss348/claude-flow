{
  "name": "@claude-flow/plugins",
  "version": "3.0.0-alpha.1",
  "description": "Unified Plugin SDK for Claude Flow V3 - Worker, Hook, and Provider Integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk/index.d.ts",
      "import": "./dist/sdk/index.js"
    },
    "./workers": {
      "types": "./dist/workers/index.d.ts",
      "import": "./dist/workers/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "events": "^3.3.0"
  },
  "peerDependencies": {
    "@claude-flow/hooks": "^3.0.0-alpha.1",
    "@claude-flow/memory": "^3.0.0-alpha.1"
  },
  "peerDependenciesMeta": {
    "@claude-flow/hooks": { "optional": true },
    "@claude-flow/memory": { "optional": true }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "claude-flow",
    "plugins",
    "sdk",
    "workers",
    "hooks",
    "agentic-flow",
    "agentdb",
    "ai-agents"
  ],
  "author": "Claude Flow Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  }
}
